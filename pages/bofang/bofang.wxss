  z-index: 0;
  margin-right: 20rpx;
}

.wangqi-img {
  width: 320rpx;
  height: 178rpx;
  border-radius: 10rpx;
}

.data {
  text-align: center;
  font-size: 12px;
  color: #fff;
  width: 150rpx;
  background-color: rgba(194, 193, 193, 0.2);
  position: absolute;
  z-index: 0;
