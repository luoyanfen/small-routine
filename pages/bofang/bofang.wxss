  line-height: 100rpx;
  color: #fff;
}

.tuijian {
  width: 680rpx;
  margin-top: 20rpx;
  border-bottom: 2rpx solid rgba(204, 204, 204, 0.3);
}

.tujian-title {
  height: 100rpx;
  color: #fff;
  font-size: 18px;
  line-height: 100rpx;
}

.tuijian-shipin {
  width: 680rpx;
  height: 450rpx;
  margin: 30rpx 0;
}

.tuijian-title {
  font-size: 16px;
  color: #fff;
}

.img-video {
  margin: 20rpx 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.tuijian-img {
  width: 195rpx;
  border-radius: 10rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.shipin-video {
  width: 480rpx;
