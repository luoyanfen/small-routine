@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1568774279280#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2')
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-xiaoxi:before {
  content: "\e64d";
}

.icon-fenxiang:before {
  content: "\e606";
}

.icon-sanjiaoxing:before {
  content: "\e607";
}

.icon-woxihuande:before {
  content: "\e60a";
}
.size {
  font-size: 24px;
  color: #fff;
  font-weight: bolder;
}

.top {
  width: 100%;
  height: 130rpx;
  position: fixed;
  color: #fff;
  background-color: #2a3c4d;
  z-index: 1000;
}

.top-top {
  box-sizing: border-box;
  display: flex;
  height: 130rpx;
  font-size: 18px;
  padding-top: 65rpx;
}

.back {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  font-size: 20px;
  font-weight: bolder;
  margin-left: 20rpx;
  margin-right: 200rpx;
}

.shipin {
  padding-top: 130rpx;
  width: 750rpx;
}

.shipin-bofang {
  width: 750rpx;
  height: 660rpx;
}

.bofang-top {
  width: 750rpx;
  height: 421rpx;
  position: relative;
  z-index: 0;
}

.tu {
  width: 750rpx;
}

.bofang {
  width: 750rpx;
  height: 425rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(130, 130, 130, 0.6);
  position: absolute;
  z-index: 0;
  top: 0;
}

.bogang-button {
  font-size: 16px;
  width: 360rpx;
  height: 75rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 33rpx;
  background-color: #4a5c6d;
}

.icon {
  color: #fff;
}

.liuliang {
  color: #fff;
  margin: 0 15rpx;
}

.color {
  display: inline-block;
  color: #f40;
}

.shipin-bottom {
  height: 240rpx;
  width: 720rpx;
  padding-left: 30rpx;
  background-color: #2a3c4d;
}

.title {
  color: #fff;
  font-size: 18px;
  height: 100rpx;
  line-height: 100rpx;
}

.tubiao {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.size1 {
  margin-left: 410rpx;
}
.size2 {
  margin-right: 30rpx;
}
.shipin-bottom text {
  font-size: 14px;
  color: #ccc;
}

.main-bottom {
  padding-left: 30rpx;
  background-color: #2a3c4d;
}

/* 专辑列表 */

.zhuanji {
  width: 720rpx;
  background-color: #2a3c4d;
  border-top: 2rpx solid rgba(204, 204, 204, 0.3);
  border-bottom: 2rpx solid rgba(204, 204, 204, 0.3);
}

.zhuanji-top {
  width: 100%;
  height: 100rpx;
  font-size: 18px;
  line-height: 100rpx;
  position: relative;
  display: flex;
}

.zhuanji-title {
  color: #fff;
}

.zhuanji-xq {
  color: #ccc;
  position: absolute;
  right: 0;
}

.back1 {
  display: inline-block;
  width: 50rpx;
  margin: 0 10rpx;
}

.zhuanji-items {
  height: 210rpx;
  display: flex;
}

.zhuanji-item {
  font-size: 14px;
  box-sizing: border-box;
  height: 165rpx;
  width: 320rpx;
  padding: 20rpx;
  margin-left: 20rpx;
  background-color: rgba(194, 193, 193, 0.2);
  border-radius: 10rpx;
  color: #ccc;
  flex-shrink: 0;
}

.wangqi {
  width: 720rpx;
  background-color: #2a3c4d;
  border-bottom: 2rpx solid rgba(204, 204, 204, 0.3);
}

.wangqi-title {
