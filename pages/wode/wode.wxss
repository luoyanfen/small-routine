.container {
  height: 600rpx;
  width: 100%;
  background-color: #fff;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 0 20rpx;
  border-radius:50%;
}

.userinfo-nickname {
  color: #aaa;
}

.box {
  background-color: rgb(236, 236, 241);
  width: 100%;
}

.vip {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  line-height: 150rpx;
  margin: 10rpx 0;
  border-bottom: solid rgb(236, 236, 241) 30rpx;
  display: flex;
}

.more {
  margin-left:434rpx;
}
.image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 50rpx;
  margin-left: 20rpx;
}
.word{
 margin-left: 20rpx;
 font-weight: 100;
}

.history {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  line-height: 150rpx;
  font-size: 18px;
  display: flex;
  justify-content: space-between;
}

.history span {
  margin: 0 30rpx;
}

.look {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  line-height: 150rpx;
  font-size: 18px;
  border: solid rgb(236, 236, 241);
